package example;
base (
	m = "m";
	s = "s";
)

derived (
	h = s * 3600;
	km = 1000 * m;
  kmph = km / h;
  nm = 1852 * m;
  kn = nm / h;
)

quantity (
  Scalar();
  Length(m);
  Area(m * m);
  Time(s);
	Frequency(s^-1);
	Distance(km);
	Duration(h);
  Speed(kmph);
)

operation (
  Area = Length * Length;
  Length = Length * Scalar;
  Time = Time * Scalar;
  Speed = Distance / Duration;
  Distance = Speed * Duration;
  Frequency = Scalar / Time;
)
