package example;
unit (
  scalarUnit = "";
  timeUnit = "s";
  lengthUnit = "m";
  speedUnit = "m * s^-1";
  frequencyUnit = "s^-1";
  areaUnit = "m^2";
)

quantity (
  Scalar(scalarUnit);
  Length(lengthUnit);
  Time(timeUnit);
  Speed(speedUnit);
  Frequency(frequencyUnit);
  Area(areaUnit);
)

operation (
  Area = Length * Length;
  Length = Length * Scalar;
  Time = Time * Scalar;
  Speed = Length / Time;
  Length = Speed * Time;
  Frequency = Scalar / Time;
)
