package example;
base (
	m = "m";
	s = "s";
)

derived (
	mps = m / s;
)

quantity (
  Scalar();
  Length(m);
  Time(s);
  Speed(mps);
  Frequency(s^-1);
  Area(m * m);
)

operation (
  Area = Length * Length;
  Length = Length * Scalar;
  Time = Time * Scalar;
  Speed = Length / Time;
  Length = Speed * Time;
  Frequency = Scalar / Time;
)
